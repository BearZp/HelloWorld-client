{
  "name": "helloworld/client",
  "description": "This is client for helloWorld services",
  "minimum-stability": "stable",
  "license": "proprietary",
  "type": "library",
  "authors": [
    {
      "name": "BearZp",
      "email": "gulidov.vadim@gmail.com"
    }
  ],
  "keywords": [ "HelloWorld", "Client" ],
  "autoload": {
    "psr-4": {
      "Client\\": "/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "": "/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
