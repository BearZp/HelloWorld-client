    "helloworld/lib": "*",
    "symfony/http-client": "^5.1"
  },
  "config": {
    "github-oauth": {

    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "git",
      "url": "https://github.com/BearZp/HelloWorld-lib.git"
    }
  ]
